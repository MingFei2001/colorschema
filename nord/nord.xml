<colorScheme name="Nord" type="dark" semanticHighlighting="true">
  <colors>
    <focusBorder>#3b4252</focusBorder>
    <foreground>#d8dee9</foreground>
    <background>#2e3440</background>
    <border>#3b4252</border>
    <activityBar>
      <background>#2e3440</background>
      <foreground>#d8dee9</foreground>
      <activeBorder>#88c0d0</activeBorder>
      <activeBackground>#3b4252</activeBackground>
      <badge>
        <background>#88c0d0</background>
        <foreground>#2e3440</foreground>
      </badge>
    </activityBar>
    <button>
      <background>#88c0d0ee</background>
      <foreground>#2e3440</foreground>
      <hoverBackground>#88c0d0</hoverBackground>
      <secondaryBackground>#434c5e</secondaryBackground>
      <secondaryForeground>#d8dee9</secondaryForeground>
      <secondaryHoverBackground>#4c566a</secondaryHoverBackground>
    </button>
    <chart>
      <red>#bf616a</red>
      <blue>#81a1c1</blue>
      <yellow>#ebcb8b</yellow>
      <orange>#d08770</orange>
      <green>#a3be8c</green>
      <purple>#b48ead</purple>
      <foreground>#d8dee9</foreground>
      <lines>#88c0d0</lines>
    </chart>
    <editor>
      <background>#2e3440</background>
      <foreground>#d8dee9</foreground>
      <cursor>#d8dee9</cursor>
      <lineNumber>
        <foreground>#4c566a</foreground>
        <activeForeground>#d8dee9</activeForeground>
      </lineNumber>
      <selection>
        <background>#434c5ecc</background>
        <highlightBackground>#434c5ecc</highlightBackground>
      </selection>
      <lineHighlight>#3b4252</lineHighlight>
      <wordHighlight>
        <background>#81a1c166</background>
        <strongBackground>#81a1c199</strongBackground>
      </wordHighlight>
      <findMatch>
        <background>#88c0d066</background>
        <highlightBackground>#88c0d033</highlightBackground>
      </findMatch>
      <inactiveSelection>#434c5ecc</inactiveSelection>
    </editor>
    <list>
      <activeSelection>
        <background>#88c0d0</background>
        <foreground>#2e3440</foreground>
      </activeSelection>
      <hoverBackground>#3b4252</hoverBackground>
      <focusBackground>#88c0d099</focusBackground>
      <highlightForeground>#88c0d0</highlightForeground>
      <errorForeground>#bf616a</errorForeground>
      <warningForeground>#ebcb8b</warningForeground>
    </list>
    <notification>
      <background>#3b4252</background>
      <foreground>#d8dee9</foreground>
      <error>
        <background>#bf616a</background>
        <foreground>#2e3440</foreground>
      </error>
      <info>
        <background>#88c0d0</background>
        <foreground>#2e3440</foreground>
      </info>
      <warning>
        <background>#ebcb8b</background>
        <foreground>#2e3440</foreground>
      </warning>
    </notification>
    <statusBar>
      <background>#3b4252</background>
      <foreground>#d8dee9</foreground>
      <debugging>
        <background>#5e81ac</background>
        <foreground>#d8dee9</foreground>
      </debugging>
    </statusBar>
    <tab>
      <activeBackground>#3b4252</activeBackground>
      <activeForeground>#d8dee9</activeForeground>
      <inactiveBackground>#2e3440</inactiveBackground>
      <inactiveForeground>#d8dee966</inactiveForeground>
      <hoverBackground>#3b4252cc</hoverBackground>
    </tab>
    <terminal>
      <background>#2e3440</background>
      <foreground>#d8dee9</foreground>
      <ansi>
        <black>#3b4252</black>
        <red>#bf616a</red>
        <green>#a3be8c</green>
        <yellow>#ebcb8b</yellow>
        <blue>#81a1c1</blue>
        <magenta>#b48ead</magenta>
        <cyan>#88c0d0</cyan>
        <white>#e5e9f0</white>
        <brightBlack>#4c566a</brightBlack>
        <brightRed>#bf616a</brightRed>
        <brightGreen>#a3be8c</brightGreen>
        <brightYellow>#ebcb8b</brightYellow>
        <brightBlue>#81a1c1</brightBlue>
        <brightMagenta>#b48ead</brightMagenta>
        <brightCyan>#88c0d0</brightCyan>
        <brightWhite>#e5e9f0</brightWhite>
      </ansi>
    </terminal>
    <scrollbar>
      <shadow>#00000066</shadow>
      <slider>
        <background>#434c5e99</background>
        <hoverBackground>#434c5eaa</hoverBackground>
      </slider>
    </scrollbar>
    <editorGroup>
      <background>#2e3440</background>
      <border>#3b425201</border>
      <dropBackground>#3b425299</dropBackground>
    </editorGroup>
    <peekView>
      <background>#2e3440</background>
      <border>#4c566a</border>
      <result>
        <background>#2e3440</background>
        <fileForeground>#88c0d0</fileForeground>
        <lineForeground>#d8dee966</lineForeground>
      </result>
      <title>
        <background>#3b4252</background>
        <descriptionForeground>#d8dee9</descriptionForeground>
        <labelForeground>#88c0d0</labelForeground>
      </title>
    </peekView>
  </colors>
</colorScheme>
